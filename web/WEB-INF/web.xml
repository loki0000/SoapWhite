<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_4_0.xsd"
           version="4.0">

    <servlet>
        <servlet-name>adiministrator</servlet-name>
        <servlet-class>cn.javafeizhao.web.ServletAdiministratorWEB</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>adiministrator</servlet-name>
        <url-pattern>/adiministrator</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>showproduct</servlet-name>
        <servlet-class> cn.javafeizhao.web.ServletShowProduct</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>showproduct</servlet-name>
        <url-pattern>/showproduct</url-pattern>
    </servlet-mapping>



</web-app>
